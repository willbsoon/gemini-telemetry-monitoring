{
  "title": "Gemini CLI 실시간 모니터링 (Token & Session)",
  "tags": [
    "gemini",
    "llm",
    "tokens",
    "latency",
    "cost"
  ],
  "timezone": "browser",
  "refresh": "5s",
  "schemaVersion": 30,
  "templating": {
    "list": [
      {
        "name": "gen_ai_request_model",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(gen_ai_client_token_usage_sum, gen_ai_request_model)",
        "refresh": 1,
        "includeAll": true,
        "multi": true,
        "allValue": ".*"
      },
      {
        "name": "session_id",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(gemini_cli_api_request_count_total, session_id)",
        "refresh": 1,
        "includeAll": true,
        "multi": true,
        "allValue": ".*"
      }
    ]
  },
  "panels": [
    {
      "title": "기간 내 총 토큰 사용량 (Total Tokens)",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 7,
        "w": 8,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(increase(gen_ai_client_token_usage_sum{session_id=~\"$session_id\", gen_ai_token_type=\"input\"}[$__range]))",
          "legendFormat": "총 입력 토큰",
          "refId": "A",
          "instant": true
        },
        {
          "expr": "sum(increase(gen_ai_client_token_usage_sum{session_id=~\"$session_id\", gen_ai_token_type=\"output\"}[$__range]))",
          "legendFormat": "총 출력 토큰",
          "refId": "B",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": []
      }
    },
    {
      "title": "세션별 누적 토큰 사용량 비교 (Bar Chart)",
      "type": "barchart",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 7,
        "w": 16,
        "x": 8,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by (short_session_id) (label_replace(increase(gen_ai_client_token_usage_sum{session_id=~\"$session_id\"}[$__range]), \"short_session_id\", \"$1\", \"session_id\", \".*?(.{6})$\"))",
          "legendFormat": "{{short_session_id}}",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "orientation": "auto",
        "xTickLabelRotation": 0,
        "xTickLabelMaxLength": 0,
        "xTickLabelSpacing": 0,
        "showValue": "auto",
        "stacking": "none",
        "tooltip": {
          "mode": "single"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "lineWidth": 1,
            "fillOpacity": 80,
            "gradientMode": "none"
          },
          "unit": "short"
        },
        "overrides": []
      }
    },
    {
      "title": "세션별 실시간 입력 토큰 (Tokens/s)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 0,
        "y": 13
      },
      "targets": [
        {
          "expr": "sum by (short_session_id) (label_replace(rate(gen_ai_client_token_usage_sum{session_id=~\"$session_id\", gen_ai_token_type=\"input\"}[1m]), \"short_session_id\", \"$1\", \"session_id\", \".*?(.{6})$\"))",
          "legendFormat": "Session: {{short_session_id}}",
          "refId": "A"
        },
        {
          "expr": "vector(0)",
          "legendFormat": "Baseline",
          "refId": "Baseline"
        }
      ],
      "options": {
        "tooltip": {
          "mode": "multi"
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 10,
            "lineWidth": 2
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Baseline"
            },
            "properties": [
              {
                "id": "custom.hideFrom",
                "value": {
                  "legend": true,
                  "tooltip": true,
                  "viz": false
                }
              },
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "transparent"
                }
              }
            ]
          }
        ]
      }
    },
